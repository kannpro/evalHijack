{
    "name": "evalHijack",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": [ "<all_urls>" ],
        "run_at": "document_end",
        "all_frames": true,
        "js": ["content_script.js"]
      }
    ],
    "web_accessible_resources": [
        "hijack.js"
    ]
}